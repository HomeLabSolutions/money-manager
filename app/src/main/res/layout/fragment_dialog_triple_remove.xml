<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="280dp"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/triple_remove_dialog_title"
        style="@style/Theme.MoneyManager.MaterialDialog.Title.Text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/standard_left_margin"
        android:paddingTop="16dp"
        android:paddingEnd="@dimen/standard_right_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/transaction_delete_dialog_title" />

    <TextView
        android:id="@+id/triple_remove_dialog_subtitle"
        style="@style/Theme.MoneyManager.MaterialDialog.Body.Text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/standard_left_margin"
        android:paddingTop="16dp"
        android:paddingEnd="@dimen/standard_right_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/triple_remove_dialog_title"
        android:text="@string/sub_category_delete_subtitle" />

    <TextView
        android:id="@+id/triple_remove_dialog_button_action1"
        style="@style/Theme.MoneyManager.MaterialDialog.Button.Text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/triple_remove_dialog_subtitle"
        tools:text="action1" />

    <TextView
        android:id="@+id/triple_remove_dialog_button_action2"
        style="@style/Theme.MoneyManager.MaterialDialog.Button.Text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/triple_remove_dialog_button_action1"
        tools:text="action2" />

    <TextView
        android:id="@+id/triple_remove_dialog_button_cancel"
        style="@style/Theme.MoneyManager.MaterialDialog.Button.Text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/triple_remove_dialog_button_action2"
        tools:text="@string/cancel" />
</androidx.constraintlayout.widget.ConstraintLayout>
